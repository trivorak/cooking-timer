{
  "name": "cooking-timer",
  "version": "1.0.0",
  "description": "A timer with the ability to save user presets(time) for cooking different dishes.",
  "main": "cooking-dishes.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "mkdir -p dist && browserify -t [ babelify --presets [ es2015 stage-0 ] ] app.js > dist/bundle.js",
    "watch": "watchify app.js -o dist/bundle.js | wtch dist/bundle.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/trivorak/cooking-timer.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/trivorak/cooking-timer/issues"
  },
  "homepage": "https://github.com/trivorak/cooking-timer#readme",
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "babelify": "^7.3.0",
    "browserify": "^13.0.1",
    "watchify": "^3.7.0",
    "wtch": "^4.0.1"
  }
}
